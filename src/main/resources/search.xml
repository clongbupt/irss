<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="search" extends="struts-default">
        <action name="search" method="search" class="SearchAction">
            <result name="success">/jsp/search/result.jsp</result>
    	</action> 	
    	
    	<action name="detail" method="detail" class="SearchAction">
            <result name="success">/jsp/search/detail.jsp</result>
    	</action> 	 
    </package>
    
</struts>
