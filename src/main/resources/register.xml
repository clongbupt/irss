<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <package name="register" extends="struts-default">
        <action name="register" method="register" class="registerAction">
            <result name="register">/jsp/user/register.jsp</result>
            <result name="error">/jsp/user/register.jsp</result>
            <result name="success">/jsp/user/registersuccess.jsp</result>
    	</action> 		 
    </package>
    
</struts>
